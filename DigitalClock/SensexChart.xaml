<Window x:Class="CommonFrontEnd.View.DigitalClock.SensexChart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommonFrontEnd.View.DigitalClock"
        mc:Ignorable="d"
        xmlns:rg="clr-namespace:Righthand.RealtimeGraph;assembly=Righthand.RealtimeGraph"
        Title="SensexChart" Height="300" Width="300"
           xmlns:vsm="clr-namespace:System.Windows;assembly=WPFToolkit" ShowInTaskbar="False">
    <Grid>
        <rg:RealtimeGraphControl x:Name="Graph" AxisColor="Blue" AxisWidth="1" VerticalLinesInterval="5" VerticalLabelsStep="2"  SpanX="100"
                             MaxY="250" MinY="50"  HorizontalLinesInterval="10"/>
    </Grid>
</Window>
